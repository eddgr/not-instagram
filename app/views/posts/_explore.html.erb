<!-- All Posts -->
<h1 class="text-center mt-4 mb-4">Explore</h1>
<div class="row">
  <% @photos.each do |post| %>
    <div class="col-4 mb-4"><%= link_to image_tag(post.photo, class: "rounded img-fluid", size: "400x400"), post %></div>
  <% end %>
</div>

<div class="text-center mb-4">
  <%= link_to "Load more", explore_path, class: "btn btn-outline-primary" %>
</div>
